//Given a column title as appears in an Excel sheet, return its corresponding column number. i.e A->1 ,B->2...Z->26,AB->27...

import java.util.*;
import java.lang.*;

class IntBit {
        static int titleToNumber(String A) {
        int res=0;
        for(int i=0;i<A.length();i++){
            res=res*26;                    //If length of the string is more than 1 
            res+=(int)A.charAt(i)-64;      //Ascii value of A is 65 so subtracting 64 from it will give A=1
        }
        return res;
    }
    
    public static void main(String[] args) {
    	Scanner in = new Scanner(System.in);
    	String a=in.nextLine();
    	int result=titleToNumber(a);
    	System.out.println(result);  
    	  }
}
